meta {
  name: Henter test statistikker
  type: http
}

get {
  url: {{baseUrl}}/api/BrunoTestResults/stats
  body: none
  auth: none
}

headers {
  Content-Type: application/json
}

tests {
  test("Status kode skal være 200 OK", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response skal være et objekt", function() {
    expect(res.getBody()).to.be.an('object');
  });
  
  test("Response skal have test statistikker", function() {
    expect(res.getBody()).to.have.property('totalFiles');
    expect(res.getBody()).to.have.property('jsonFiles');
    expect(res.getBody()).to.have.property('htmlFiles');
    expect(res.getBody()).to.have.property('totalSize');
  });
  
  test("Statistikker skal være tal", function() {
    expect(res.getBody().totalFiles).to.be.a('number');
    expect(res.getBody().jsonFiles).to.be.a('number');
    expect(res.getBody().htmlFiles).to.be.a('number');
    expect(res.getBody().totalSize).to.be.a('number');
  });
  
  test("Response tid skal være under 500ms", function() {
    expect(res.getResponseTime()).to.be.below(500);
  });
}
